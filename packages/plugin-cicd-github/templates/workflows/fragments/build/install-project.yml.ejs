<% if (('undefined' !== typeof needRoot) && !!needRoot) { -%>
      - name: Install ROOT
        shell: 'script -q -e -c "bash {0}"'
        run: make install-root
        env:
          FORCE_COLOR: 3
          CI: true
<% } -%>
      - name: Install
        shell: 'script -q -e -c "bash {0}"'
        run: make install-<%- project %>
        env:
          FORCE_COLOR: 3
          CI: true